{
    "name": "unbundled",
    "version": "0.0.1",
    "description": "This is an attempt at unbundling the web.",
    "main": "index.js",
    "scripts": {
        "start": "parcel --no-cache ./src/index.html",
        "test": "echo \"Error: no test specified\" ",
        "precommit": "precise-commits --whitelist=\"**/!(*.json)\"",
        "docs:build": "cd website && npm run build",
        "build": "parcel build ./src/index.html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/humphd/next.git"
    },
    "author": "David Humphrey",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/humphd/next/issues"
    },
    "homepage": "https://github.com/humphd/next#readme",
    "devDependencies": {
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-runtime": "^6.26.0",
        "husky": "^0.14.3",
        "parcel-bundler": "^1.8.1",
        "precise-commits": "^1.0.2",
        "prettier": "^1.12.1",
        "docusaurus": "^1.0.15"
    },
    "dependencies": {
        "dexie": "^2.0.3",
        "parcel-plugin-sw-cache": "dsych/parcel-plugin-sw-cache#single-regex"
    },
    "cache": {
        "strategy": "inject",
        "globDirectory": "src/",
        "globPatterns": [
            "**/*.{js,html}"
        ],
        "swSrc": "dist/db-worker.js",
        "swDest": "dist/db-worker.js"
    }
}
